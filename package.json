{
  "name": "squri",
  "version": "0.1.0",
  "description": "A unified codebase framework for building desktop and mobile apps, powered by Quasar (UI layer) and Tauri (system layer)",
  "productName": "Squri App",
  "author": "andeya <andeyalee@outlook.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint -c ./eslint.config.js \"./src/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:icon": "tauri icon src-tauri/icons/icon.svg -o src-tauri/icons",
    "android:init": "tauri android init",
    "android:dev": "tauri android dev",
    "android:build": "tauri android build",
    "ios:init": "tauri ios init",
    "ios:dev": "tauri ios dev",
    "ios:build": "tauri ios build",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-http": "^2.5.2",
    "@tauri-apps/plugin-notification": "^2.3.0",
    "@tauri-apps/plugin-os": "^2.3.2",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "@vueuse/core": "^14.1.0",
    "pinia": "^3.0.4",
    "quasar": "^2.18.6",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@quasar/app-vite": "^2.4.0",
    "@quasar/cli": "^2.5.0",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^22.15.21",
    "@vue/eslint-config-prettier": "^10.1.8",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.4.0",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.7.4",
    "sass": "^1.96.0",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^7.2.7",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
